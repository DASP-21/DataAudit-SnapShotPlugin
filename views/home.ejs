<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <title>DASP Service</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Montserrat&amp;display=swap">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <script>
        const toggleDisplay = () => {
            let table = document.getElementById('table-body');
            const input_data = document.getElementById('input-data');
            if (!input_data.value.length) {
                alert('Invalid Search Query!');
            } else if (!(input_data.value === '13011, 13015, 13017')) {
                alert('Data ID Not Present in the Database');
            } else {
                console.log("worked");
                table.classList.toggle("hidden");
            }
        }

    </script>
    <style>
        .hidden {
            display: none;
        }
    </style>
</head>

<body>
    <nav class="navbar navbar-light navbar-expand" style="background-color: #1D3845">
        <div class="container"><a class="navbar-brand" href="#"
                style="font-family: Montserrat,sans-serif;font-size: 25px;color: rgba(255,255,255,0.9);">
                DASP - Service</a><button class="navbar-toggler" data-bs-toggle="collapse"></button></div>
    </nav>
    <header class="text-center text-black masthead"
        style="background: url('bg(1).jpg')no-repeat center center; background-size: cover;height: 865px;">
        <div class="container">
            <div class="row" style="margin-top: 0px; padding-top: 250px;">
                <div class="col-xl-9 mx-auto position-relative"
                    style="background-color: #262626; color: #fff; padding: 5px; width: 831px; height: 250px; border-radius: 20px; box-shadow: 50px 5px 200px 5px #404040;">
                    <h1 class="mb-5 mt-4"
                        style="font-size: 74px;font-family: Montserrat, sans-serif; font-weight: bold">DASP - Service
                    </h1>
                    <div class="row">
                        <div class="col">
                            <h1 style="font-size: 30px;font-family: Montserrat, sans-serif;">Your One Step CDC Solution
                            </h1>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <section class=" features-icons"
        style="padding-top: 50px; padding-bottom: 50px; margin-top: 50px; margin-bottom: 50px; padding-left: 100px;">
        <div class="container" style="width: 700px;">
            <div class="row text-center" style="width: 600px;">
                <div class="col">
                    <h4 style="margin-top: 1rem; font-family: Montserrat, sans-serif; font-size: larger;">Search for
                        your Registered Log
                    </h4>
                </div>
                <div class="input-group col">
                    <input type="search" class="form-control rounded"
                        style="font-family: Montserrat, sans-serif; width: 100px;" placeholder="Search"
                        aria-label="Search" aria-describedby="search-addon" id="input-data" />
                    <button id="search-button" type="button" class="btn btn-primary"
                        style="background-color: #1D3845; font-family: Montserrat, sans-serif;"
                        onclick="toggleDisplay()">
                        GO
                    </button>
                </div>
            </div>
        </div>
    </section>
    <section style="padding-bottom: 50px; margin-top: 50px; margin-bottom: 50px;" class="table-body">
        <div class="container hidden" style="width: 1200px;" id="table-body">
            <table class="table text-center">
                <thead class="thead-dark text-white" style="background-color: #1D3845;">
                    <tr>
                        <th scope="col">#</th>
                        <th scope="col">Data_ID</th>
                        <th scope="col">Current State</th>
                        <th scope="col">Captured History</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <th scope="row">1</th>
                        <td>13011</td>
                        <td>Content Latest</td>
                        <td>[content v1, content v2, ...]</td>
                    </tr>
                    <tr>
                        <th scope="row">2</th>
                        <td>13015</td>
                        <td>Employee Latest</td>
                        <td>[Employee v1, Employee v2, ...]</td>
                    </tr>
                    <tr>
                        <th scope="row">3</th>
                        <td>13017</td>
                        <td>Invoice Txn Latest</td>
                        <td>[Invoice v1, Invoice v2, ...]</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </section>
    <section class="text-center features-icons" style="padding-bottom: 50px; margin-top: 50px; padding-left: 100px;">
        <div class="container">
            <div class="row">
                <div class="col-lg-4" style="width: 600px;">
                    <p
                        style="text-align: left; font-family: Montserrat, sans-serif; font-size: 35px; margin-bottom: 30px; padding-top: 50px;">
                        Services offered</p>
                    <div id="Bullets">
                        <ul class="c-bullets">
                            <li
                                style="text-align: left;font-family: Montserrat, sans-serif;font-size: 21px; padding-top: 10px;">
                                To register any data item, make a request to <code
                                    style="background-color: #e6e6e6;"> /api/createlog</code>.<br></li>
                            <li
                                style="text-align: left;font-family: Montserrat, sans-serif;font-size: 21px; padding-top: 10px;">
                                Already registered and want to update state of data item, make a request to <code
                                    style="background-color: #e6e6e6;"> /api/createlog</code> with new attributes.<br>
                            </li>
                            <li
                                style="text-align: left;font-family: Montserrat, sans-serif;font-size: 21px; padding-top: 10px;">
                                To check current state of data item with last updated time stamp, make request to <code
                                    style="background-color: #e6e6e6;"> /api/getlog</code>.
                            </li>
                            <li
                                style="text-align: left;font-family: Montserrat, sans-serif;font-size: 21px; padding-top: 10px;">
                                If you want to compare your capture history and current data state simply make a request
                                to <code style="background-color: #e6e6e6;">/api/getcapturehistory</code>.
                            </li>
                        </ul>
                    </div>
                </div>
                <div class="col" style="padding-top: 120px; padding-left: 70px;"><img
                        style="width: 436px;height: 300px; border: 1px solid #999999; border-radius: 20px; padding: 5px; width: 436px; box-shadow: rgba(17, 17, 26, 0.1) 0px 8px 24px, rgba(17, 17, 26, 0.1) 0px 16px 56px, rgba(17, 17, 26, 0.1) 0px 24px 80px;"
                        src="carbon.png" alt="DASP"></div>
            </div>
        </div>
    </section>
    <section class="text-center  testimonials" style="padding: 50px 0px;padding-top: 100px;">
        <div class="container">
            <h2 class="mb-5"
                style="font-family: Montserrat, sans-serif; font-size: 35px; text-align: left; padding-left: 65px; padding-bottom: 20px;">
                OUR TEAM</h2>
            <div class="row" style="padding-left: 100px;">
                <div class="col">
                    <div class="mx-auto testimonial-item mb-5 mb-lg-0">
                        <h5 style="font-family: Montserrat, sans-serif;">Akash Chouhan</h5>
                        <p class="font-weight-light mb-0" style="font-family: Montserrat, sans-serif;">1905156</p>
                    </div>
                </div>
                <div class="col">
                    <div class="mx-auto testimonial-item mb-5 mb-lg-0">
                        <h5 style="font-family: Montserrat, sans-serif;">Akriti Anand</h5>
                        <p class="font-weight-light mb-0" style="font-family: Montserrat, sans-serif;">1905157</p>
                    </div>
                </div>
                <div class="col">
                    <div class="mx-auto testimonial-item mb-5 mb-lg-0">
                        <h5 style="font-family: Montserrat, sans-serif;">Surya Thangirala</h5>
                        <p class="font-weight-light mb-0" style="font-family: Montserrat, sans-serif;">1905245</p>
                    </div>
                </div>
                <div class="col">
                    <div class="mx-auto testimonial-item mb-5 mb-lg-0">
                        <h5 style="font-family: Montserrat, sans-serif;">Kanishk Gupta</h5>
                        <p class="font-weight-light mb-0" style="font-family: Montserrat, sans-serif;">1905902</p>
                    </div>
                </div>
                <div class="col">
                    <div class="mx-auto testimonial-item mb-5 mb-lg-0">
                        <h5 style="font-family: Montserrat, sans-serif;">Himanshu Verma</h5>
                        <p class="font-weight-light mb-0" style="font-family: Montserrat, sans-serif;">1905900</p>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"></script>
</body>

</html>